    "\n",
    "# Configure Gemini\n",
    "genai.configure(api_key=api_key)\n",
    "\n",
    "# Find most recent with_answers.json\n",
    "WITH_ANSWERS_PATH = Path(\"runs\")  \n",
    "\n",
    "candidates = sorted(\n",
    "    WITH_ANSWERS_PATH.glob(\"*/with_answers.json\"),\n",
    "    key=lambda p: p.stat().st_mtime,\n",
    "    reverse=True,\n",
    ")\n",
    "if not candidates:\n",
    "    raise FileNotFoundError(\"No runs/*/with_answers.json found. Set DATA_PATH manually.\")\n",
    "DATA_PATH = candidates[0]\n",
    "RUN_DIR   = DATA_PATH.parent\n",
    "\n",
    "print(\"Using:\", DATA_PATH)\n",
    "\n",
    "data = json.loads(DATA_PATH.read_text(encoding=\"utf-8\"))\n",
    "items: List[Dict[str, Any]] = data.get(\"outputs\", [])\n",
    "print(\"Items loaded:\", len(items))\n",
    "\n",
    "# Count judgeable items (exclude errors and filtered responses)\n",
    "judgeable = sum(1 for it in items if (\n",
    "    (it.get(\"model_answer\") or \"\").strip() and \n",
    "    not it[\"model_answer\"].strip().startswith(\"<ERROR\") and\n",
    "    not it[\"model_answer\"].strip().startswith(\"<FILTERED\")\n",
    "))\n",
    "print(f\"Judgeable answers: {judgeable}/{len(items)}\")\n"
